#include<stdio.h>

int main(){
    int height,width;

    scanf("%d %d", &height, &width);

    for(int i=0;i<height;i++){
        printf(" __");
        if(i==height-1){
          printf(" __");
        }
    }

    printf("\n");
    
    for(int i=0; i<width; i++){
        for(int j=0; j<height; j++){
        printf("|__");
        if(j==height-1){
          printf("|__|");
        }
      
      }
      printf("\n");
    }
}
